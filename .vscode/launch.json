  "version": "0.2.0",

  "configurations": [
    {
      "name": "Run GgCheck",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/bin/gg_check.dart",
      "args": ["ggCheck", "--param", "value"],
      "console": "debugConsole"
    },
    {
      "name": "Test Current File",
      "type": "dart",
      "request": "launch",
      "program": "${file}"
    }
  ]
}